package JavaPackage;

public class MultidimentionArray {

	public static void main(String[] args) {

 int a [] [] = {{0,1,3,5}, {1,2,3,4}, {4,5,9,10}};
 
  //Number of rows
 System.out.println("rows of array is " +a.length);
 //Number of column
 System.out.println("Column of array " +a[0].length);
 
  for (int i =0 ;i< a.length;i++)
  {
    for (int j =0; j< a[0].length; j++)
    {
  System.out.print(a[i][j] +" ");
	}
  }
}
}