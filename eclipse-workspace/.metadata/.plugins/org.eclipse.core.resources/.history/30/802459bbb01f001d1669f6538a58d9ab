package JavaPackage;

public class Operator {

	public static void main(String[] args) {

 int a =10 , b = 5;
 
 //Assigment
  System.out.println(a+=b);
  System.out.println(a-=b);
  System.out.println(a*=b);
  System.out.println(a/=b);
  //Arithgmetic
  System.out.println(a/b);
  System.out.println(a+b);
  System.out.println(a-b);
  System.out.println(a*b);
  
  //relational
  if (a>b)
  {
	  System.out.println( "a is bigger");
  
  }
		 
		 

	}

}
