package javaPackage;

import java.util.Arrays;

public class WhatwilbeOutput {
	
	
	public static void main(String[] args) {
		
int []a = {10,9,7,9};
int []b= {2,6,4,3};
 int l1=a.length;
 int l2=b.length;
 
 int []res= new int [l1+l2];
 
 System.arraycopy(a, 0, res, 0, l1);
 System.arraycopy(b, 0, res, l1, l2);
 
 for(int i = 0;i< res.length;i++) {
	 System.out.println(res);
 }
 
 
 
		
				
		
		
		
		
				
				
		
		
		
		
		
	}
	
	
	}
	