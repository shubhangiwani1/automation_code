package javaPackage;




public class Wahtwillbeoutput {
	
	public static void main(String[] args) throws Exception {
		String s = "radar **  &";
		s= s.replaceAll("[*& ]", "");

		String reverse ="";

		for (int i= s.length()-1; i>=0; i--){

		 reverse = reverse+ s.charAt(i);

		}

		System.out.println(s);
		
	}}
  
	       